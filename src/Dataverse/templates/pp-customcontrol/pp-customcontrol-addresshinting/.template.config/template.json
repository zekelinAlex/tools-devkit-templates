{
  "$schema": "http://json.schemastore.org/template",
  "author": "NETWORG",
  "identity": "TALXIS.DevKit.Templates.Dataverse.UI.Controls.CustomControl.Addresshinting",
  "name": "Power Platform: AddressHinting",
  "shortName": "pp-customcontrol-addresshinting",
  "sourceName": "examplecustomentityattribute",
  "preferNameDirectory": false,
  "tags": {
    "language": "XML",
    "type": "item"
  },
  "symbols": {
    "CustomControl_fullAddress": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "fulladdressexampletypesinglelinetext",
      "fileRename": "fulladdressexampletypesinglelinetext",
      "description": "Full address attribute to be bounded to.",
      "isRequired": true
    },
    "CustomControl_addressLanguage": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "user-default"
        },
        {
          "choice": "en"
        }
      ],
      "description": "Sometimes you might want to specify the language in which the address should be returned. (user-default: This option defaults to users personal settings.; en: English language.)",
      "isRequired": true
    },
    "addresslanguagevalue": {
      "type": "generated",
      "generator": "switch",
      "replaces": "addresslanguageexampletypeenum",
      "parameters": {
        "datatype": "string",
        "cases": [
          {
            "condition": "(addressLanguage == \u0022user-default\u0022)",
            "value": "user-default"
          },
          {
            "condition": "(addressLanguage == \u0022en\u0022)",
            "value": "en"
          }
        ]
      },
      "description": "Sometimes you might want to specify the language in which the address should be returned. automatically selected based on addressLanguage"
    },
    "CustomControl_addressId": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "addressidexampletypesinglelinetext",
      "fileRename": "addressidexampletypesinglelinetext",
      "description": "Address id from the service."
    },
    "CustomControl_country": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "countryexampletypesinglelinetext",
      "fileRename": "countryexampletypesinglelinetext",
      "description": "Country of the address."
    },
    "CustomControl_countryIsoAlpha2Code": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "countryisoalpha2codeexampletypesinglelinetext",
      "fileRename": "countryisoalpha2codeexampletypesinglelinetext",
      "description": "Alpha 2 country ISO code of the address."
    },
    "CustomControl_administrativeArea": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "administrativeareaexampletypesinglelinetext",
      "fileRename": "administrativeareaexampletypesinglelinetext",
      "description": "Addministrative area of the address."
    },
    "CustomControl_locality": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "localityexampletypesinglelinetext",
      "fileRename": "localityexampletypesinglelinetext",
      "description": "Locality of the address."
    },
    "CustomControl_subLocality": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "sublocalityexampletypesinglelinetext",
      "fileRename": "sublocalityexampletypesinglelinetext",
      "description": "Sub-locality of the address."
    },
    "CustomControl_street": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "streetexampletypesinglelinetext",
      "fileRename": "streetexampletypesinglelinetext",
      "description": "Street of the address."
    },
    "CustomControl_streetNumber": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "streetnumberexampletypesinglelinetext",
      "fileRename": "streetnumberexampletypesinglelinetext",
      "description": "Street number of the address."
    },
    "CustomControl_formattedPostalCode": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "formattedpostalcodeexampletypesinglelinetext",
      "fileRename": "formattedpostalcodeexampletypesinglelinetext",
      "description": "Formatted postal code of the address."
    },
    "CustomControl_latitude": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "latitudeexampletypedecimal",
      "fileRename": "latitudeexampletypedecimal",
      "description": "Latitude (coordinates) of the address."
    },
    "CustomControl_longitude": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "longitudeexampletypedecimal",
      "fileRename": "longitudeexampletypedecimal",
      "description": "Longitude (coordinates) of the address."
    },
    "CustomControl_plusCode": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "pluscodeexampletypesinglelinetext",
      "fileRename": "pluscodeexampletypesinglelinetext",
      "description": "Open location code (OLC)."
    },
    "CustomControl_manualInput": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "manualinputexampletypetwooptions",
      "fileRename": "manualinputexampletypetwooptions",
      "description": "If the control is in manual mode, autocomplete is not working and the control is in read-only mode."
    },
    "CustomControl_rawAddressJSON": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "rawaddressjsonexampletypesinglelinetext",
      "fileRename": "rawaddressjsonexampletypesinglelinetext",
      "description": "Raw address JSON output."
    },
    "CustomControl_geospatialExpandQuery": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "defaulttemplateexample",
      "replaces": "geospatialexpandqueryexampletypesinglelinetext",
      "fileRename": "geospatialexpandqueryexampletypesinglelinetext",
      "description": "Expand query which should be passed to geospatial Data Feed request."
    },
    "CustomControl_enableManualInput": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "Disabled"
        },
        {
          "choice": "Enabled"
        }
      ],
      "description": "Enable or Disable manual input option. If it is disabled, the option Switch to manual during user input will not be displayed. (Disabled: Disabled; Enabled: Enabled)"
    },
    "enablemanualinputvalue": {
      "type": "generated",
      "generator": "switch",
      "replaces": "enablemanualinputoptionexampletypeenum",
      "parameters": {
        "datatype": "string",
        "cases": [
          {
            "condition": "(enableManualInput == \u0022Disabled\u0022)",
            "value": "0"
          },
          {
            "condition": "(enableManualInput == \u0022Enabled\u0022)",
            "value": "1"
          }
        ]
      },
      "description": "Enable or Disable manual input option. If it is disabled, the option Switch to manual during user input will not be displayed. automatically selected based on enableManualInput"
    },
    "CustomControlFormFactor": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "unknown",
      "replaces": "customcontrolformfactorexample",
      "description": "Form factor for the custom control. (This parameter is applicable in Custom control parameters)"
    },
    "ControlId": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "unknown",
      "replaces": "controlDescriptionIdexample",
      "description": "Control ID for the custom control. (This parameter is applicable in Custom control parameters)"
    },
    "PublisherPrefix": {
      "type": "parameter",
      "datatype": "text",
      "isRequired": true,
      "replaces": "examplepublisherprefix",
      "description": "Publisher prefix for the custom control"
    },
    "SolutionRootPath": {
      "type": "parameter",
      "datatype": "text",
      "replaces": "SolutionDeclarationsRoot",
      "isRequired": true,
      "description": "Path to the solution root directory"
    },
    "EntitySchemaName": {
      "type": "parameter",
      "datatype": "text",
      "replaces": "exampleentityname",
      "isRequired": false,
      "defaultValue": "unknown",
      "description": "Logical name of the entity for which the control will be created"
    },
    "FormType": {
      "type": "parameter",
      "datatype": "choice",
      "isRequired": false,
      "replaces": "formtypeexample",
      "defaultValue": "unknown",
      "choices": [
        {
          "choice": "main"
        },
        {
          "choice": "quick"
        },
        {
          "choice": "dialog"
        },
        {
          "choice": "unknown"
        }
      ],
      "description": "Type of form: main (main form), quick (quick create), dialog (dialog form), or unknown (auto-detect)"
    },
    "FormId": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": false,
      "defaultValue": "unknownFormId",
      "replaces": "formguididexample",
      "description": "GUID of the specific form where the parameters will be added"
    }
  },
  "postActions": [
    {
      "actionId": "3A7C4B45-1F5D-4A30-959A-51B88E82B5D2",
      "args": {
        "executable": "pwsh",
        "args": "-noprofile -executionpolicy bypass -File \u0022./.template.scripts/RemoveUnusedNodes.ps1\u0022 ",
        "redirectStandardOutput": "false"
      },
      "manualInstructions": [
        {
          "text": "Removing unused nodes"
        }
      ],
      "continueOnError": false,
      "description": "Removing unused nodes"
    },
    {
      "actionId": "3A7C4B45-1F5D-4A30-959A-51B88E82B5D2",
      "args": {
        "executable": "pwsh",
        "args": "-noprofile -executionpolicy bypass -File \u0022./.template.scripts/AddCustomControlParameters.ps1\u0022 ",
        "redirectStandardOutput": "false"
      },
      "manualInstructions": [
        {
          "text": "Adding new parameters to custom control"
        }
      ],
      "continueOnError": false,
      "description": "Adding new parameters to custom control"
    },
    {
      "actionId": "3A7C4B45-1F5D-4A30-959A-51B88E82B5D2",
      "args": {
        "executable": "pwsh",
        "args": "-noprofile -executionpolicy bypass -File \u0022./.template.scripts/Cleanup.ps1\u0022",
        "redirectStandardOutput": "false"
      },
      "manualInstructions": [
        {
          "text": "Removing Scripts"
        }
      ],
      "continueOnError": false,
      "description": "Removing Scripts"
    }
  ]
}